@model HobbyViewModel
<div class="jumbotron">
    <h1 class="text-center h3">@ViewBag.Title</h1>
</div>
<div class="p-5">
    @foreach (GroupMembers m in Model.TeamList.Where(x => x.GroupMembersID == Model.MemID))
    {
        <h2 class="h4">Name: @m.FullName</h2>
        <p class="lead">@m.MemberBio</p>
    }
</div>